{#
/**
 * @file
 * Theme override for local tasks (tabs).
 */
#}
{% if primary %}
  <nav class="tabs primary mb-6" role="navigation" aria-label="{{ 'Primary tabs'|t }}">
    <ul class="flex flex-wrap gap-2 border-b border-gray-200 dark:border-slate-700">
      {{ primary }}
    </ul>
  </nav>
{% endif %}
{% if secondary %}
  <nav class="tabs secondary mb-4" role="navigation" aria-label="{{ 'Secondary tabs'|t }}">
    <ul class="flex flex-wrap gap-2">
      {{ secondary }}
    </ul>
  </nav>
{% endif %}
