{#
/**
 * @file
 * Theme override for unformatted views rows.
 */
#}
{% if title %}
  <h3 class="text-xl font-semibold text-gray-900 dark:text-white mb-4">{{ title }}</h3>
{% endif %}
<div class="space-y-4">
{% for row in rows %}
  {%
    set row_classes = [
      default_row_class ? 'views-row',
    ]
  %}
  <div{{ row.attributes.addClass(row_classes) }}>
    {{ row.content }}
  </div>
{% endfor %}
</div>
